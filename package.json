{
  "name": "fitness_app",
  "version": "1.1.0",
  "description": "Full Stack React App with Authentication",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'fitness-material-kit/*'\" \"npm run client\"",
    "client": "cd fitness-material-kit && npm run start",
    "install": "cd fitness-material-kit && npm install",
    "build": "cd fitness-material-kit && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "12.10.0",
    "npm": "6.11.3"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.4",
    "redux-devtools-extension": "^2.13.8"
  },
  "dependencies": {
    "apexcharts": "^3.15.3",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "connect-mongo": "^3.2.0",
    "cheerio": "latest",
    "devextreme": "20.1.4",
    "devextreme-react": "20.1.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.14",
    "morgan": "^1.10.0",
    "omit-empty": "^1.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.13.1",
    "react-apexcharts": "^1.3.6",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "serve": "^11.3.2",
    "validator": "^13.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raej1428/project3.git"
  },
